#

# GitHub Integration
<% if @github['oauth_token'] %>
:github_oauth_token: <%= @github['oauth_token'] %>
<% else %>
:github_login: <%= @github['login'] %>
:github_password: <%= @github['password'] %>
<% end %>

<% if @github['api_endpoint'] %>
:github_api_endpoint: <%= @github['api_endpoint'] %>
:github_web_endpoint: <%= @github['web_endpoint'] %>
<% end %>

:github_ssh_repository: <%= @github['repository'] %>
:github_polling_interval_seconds: <%= @github['polling_interval'] %>

# Jenkins Integration
<% if @jenkins['login'] %>
:jenkins_login: <%= @jenkins['login'] %>
:jenkins_password: <%= @jenkins['password'] %>
<% end %>

:jenkins_url: <%= @jenkins['url'] %>
:jenkins_job_name: <%= @jenkins['test_job'] %>
:jenkins_job_timeout_seconds: <%= @jenkins['timeout'] %>
:jenkins_polling_interval_seconds: <%= @jenkins['polling_interval'] %>

<% unless @jenkins['whitelist_branches'].empty? %>
:whitelist_branches:
<% @jenkins['whitelist_branches'].each do |branch| %>
 - <%= branch %>
<% end %>
<% end %>
